{% trans_default_domain 'admin' %}
{% extends "__layout.html.twig" %}

{% block breadcrumb_items %}
    <li class="breadcrumb-item"><a href="{{ path('admin_notification') }}">{{'Notification'|trans}}</a></li>
    <li class="breadcrumb-item">{{'Parameters'|trans}}</li>
{% endblock %}

{% block content %}
	{{form_start(form)}}
		{{form_rest(form)}}
		<p>
			<input class="btn btn-submit" type="submit" value="{{'Submit'|trans}}" name="param_submit">
			<input class="btn btn-reset" type="reset" value="{{'Reset'|trans}}" name="param_reset">
		</p>
	{{form_end(form)}}
{% endblock %}
