{
    "name": "nikrou/phyxo",
    "description": "Simply share your photos",
    "homepage": "http://www.phyxo.net",
    "license": "GPL-2.0-only",
    "authors": [
	{
	    "name": "Nicolas Roudaire",
	    "email": "nikrou77@gmail.com"
	}
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "openpsa/universalfeedcreator": "^1.8",
        "pclzip/pclzip": "^2.8",
        "pelago/emogrifier": "^4.0",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/security-bundle": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "atoum/atoum": "^3.4",
        "behat/behat": "^3.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "dmore/behat-chrome-extension": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpspec/prophecy": "^1.11",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.42",
        "phpstan/phpstan-doctrine": "^0.12.19",
        "symfony/maker-bundle": "^1.21",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
	"preferred-install": {
	    "*": "dist"
	},
	"sort-packages": true,
	"bin-dir": "bin"
    },
    "autoload": {
	"psr-4": {
	    "Phyxo\\": "src/Phyxo/",
	    "App\\": "src/"
	}
    },
    "autoload-dev": {
        "psr-4": {
	    "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
	"phpstan": "phpstan analyse",
	"auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
	"symfony": {
	    "allow-contrib": false,
	    "require": "4.4.*"
	}
    }
}
