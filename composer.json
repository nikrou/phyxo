{
    "name": "nikrou/phyxo",
    "description": "Simply share your photos",
    "homepage": "http://www.phyxo.net",
    "license": "GPL-2.0-only",
    "authors": [
	{
	    "name": "Nicolas Roudaire",
	    "email": "nikrou77@gmail.com"
	}
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.9",
        "imagine/imagine": "^1.2",
        "openpsa/universalfeedcreator": "^1.8",
        "pclzip/pclzip": "^2.8",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/yaml": "5.4.*",
        "twig/cssinliner-extra": "^3.3",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "behat/mink-browserkit-driver": "^2.0",
        "friends-of-behat/mink-extension": "^2.6",
        "friends-of-behat/symfony-extension": "^2.3",
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.1",
        "phpstan/phpstan-doctrine": "^1.0",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.21",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "vimeo/psalm": "^4.7"
    },
    "config": {
	"preferred-install": {
	    "*": "dist"
	},
	"sort-packages": true,
	"bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Phyxo\\": "src/Phyxo/",
            "App\\": "src/",
            "Plugins\\": "plugins/"
        }
    },
    "autoload-dev": {
        "psr-4": {
	    "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
	"phpstan": "phpstan analyse",
	"psalm": "psalm",
	"auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
	"symfony": {
	    "allow-contrib": false,
	    "require": "5.4.*"
	}
    }
}
