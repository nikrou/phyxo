{
    "name": "nikrou/phyxo",
    "description": "Simply share your photos",
    "homepage": "http://www.phyxo.net",
    "license": "GPL-2.0-only",
    "authors": [
	{
	    "name": "Nicolas Roudaire",
	    "email": "nikrou77@gmail.com"
	}
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0.0",
        "composer/package-versions-deprecated": "^1.11.99.5",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.13.3",
        "imagine/imagine": "^1.3.2",
        "openpsa/universalfeedcreator": "^1.8.4.1",
        "pclzip/pclzip": "^2.8.2",
        "ramsey/uuid-doctrine": "^1.8.1",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.19.3",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/yaml": "5.4.*",
        "twig/cssinliner-extra": "^3.4",
        "twig/extra-bundle": "^2.12|^3.4",
        "twig/intl-extra": "^3.4.2",
        "twig/twig": "^2.12|^3.4.3"
    },
    "require-dev": {
        "behat/behat": "^3.11",
        "behat/mink-browserkit-driver": "^2.1",
        "friends-of-behat/mink-extension": "^2.7.2",
        "friends-of-behat/symfony-extension": "^2.3.1",
        "friendsofphp/php-cs-fixer": "^3.12",
        "phpspec/prophecy-phpunit": "^2.0.1",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.8.11",
        "phpstan/phpstan-doctrine": "^1.3.21",
        "rector/rector": "^0.14.6",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.47",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*",
        "vimeo/psalm": "^4.29"
    },
    "config": {
	"preferred-install": {
	    "*": "dist"
	},
	"sort-packages": true,
	"bin-dir": "bin",
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "Phyxo\\": "src/Phyxo/",
            "App\\": "src/",
            "Plugins\\": "plugins/",
            "Themes\\": "themes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
	    "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
	"phpstan": "phpstan analyse",
	"psalm": "psalm",
	"auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
	"symfony": {
            "allow-contrib": false,
            "require": "5.4.*",
            "docker": false
        }
    }
}
